{
  "compilerOptions": {
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "~Api/*": [
        "./src/Api/*"
      ],
      "~assets/*": [
        "./src/assets/*"
      ],
      "~components/*": [
        "./src/components/*"
      ],
      "~router/*": [
        "./src/router/*"
      ],
      "~views/*": [
        "./src/views/*"
      ],
      "~/*": [
        "./src/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}